{
  "name": "zetterstrom.dev",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "pnpm": ">=8.10.0"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md,yaml,yml}\"",
    "build-and-push": "./scripts/build-and-push.sh",
    "deploy-it": "./scripts/deploy.sh",
    "dev": "./scripts/start.sh",
    "dev:index": "pnpm --filter index dev",
    "dev:movies": "pnpm --filter scraper dev",
    "dev:recipes": "pnpm --filter client dev",
    "dev:stava": "pnpm --filter stava dev"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.20.0"
}
